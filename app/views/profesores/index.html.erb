<% form_tag(profesores_path, :method => :get, :autocomplete => "off") do  %>
	<%= text_field_tag :profesor_q %>
	<%= submit_tag "Enviar", :name => nil %>
<% end %>

<h1>Listing profesores</h1>

<div id="listado">
	<%= render @profesores %>
</div>
<br />

<%= link_to 'New Profesor', new_profesor_path if can? :create, Profesor  %>

<div id="next_link">
  <%= render :partial => 'shared/show_more', :object => @profesores,
              :locals => { :url => "/profesores/mas_resultados.js", :parametros => { :profesor_q => :profesor_q, :page => :page } } %>
</div>

