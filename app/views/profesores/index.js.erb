$("#listado").html("<%= escape_javascript(render @profesores)%>");

<%
	show_more = escape_javascript(render(:partial => 'shared/show_more', :object => @profesores,
            														:locals => { :url => 'profesores/mas_resultados.js',
																											:parametros => { :profesor_q => :profesor_q }
																																			}
																				)
																)

%>

$("#next_link").html("<%= show_more %>");
$("#resultados").html("<%= @profesores.total_count %>");

<% if @profesores.current_page >= @profesores.num_pages %>
  $("#next_link").css("display", "none");
<% else %>
   $("#next_link").css("display", "block");
<% end %>
