<table>
  <tr>
    <th>Votos</th> 
    <th>Titulo</th>
    <th>Descripcion</th>
    <th>Tags</th>
    <th>Creador</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @propuestas.each do |propuesta| %>
  <tr>
    <td><%= propuesta.votos %></td>
    <td><%= propuesta.titulo %></td>
    <td><%= propuesta.descripcion %></td>
    <td><%= propuesta.tags.join(", ") %></td>
    <td><%= propuesta.usuario.username  %></td>
    <td><%= link_to 'Show', campus_propuesta_path(@campus, propuesta) %></td>
    <td><%= link_to 'Edit', edit_campus_propuesta_path(@campus, propuesta) %></td>
    <td><%= link_to 'Destroy', campus_propuesta_path(@campus, propuesta), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<%= paginate @propuestas, :remote => true %>
