<%= form_for(@universidad) do |f| %>
  <%= f.error_messages %>

  <div class="field">
    <%= f.label :nombre %><br />
    <%= f.text_field :nombre %>
  </div>
  <div class="field">
    <%= f.label :abreviaciones_tokens, "Etiquetas" %><br />
    <%= f.text_field :abreviaciones_tokens, :id => "abreviaciones" %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
