<%= form_for @comentario do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :comentario %><br />
    <%= f.text_area :comentario %>
  </p>
  <p>
    <%= f.label :usuario_id %><br />
    <%= f.text_field :usuario_id %>
  </p>
  <p>
    <%= f.label :calificacion_id %><br />
    <%= f.text_field :calificacion_id %>
  </p>
  <p><%= f.submit %></p>
<% end %>
