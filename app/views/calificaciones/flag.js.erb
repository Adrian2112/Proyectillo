<% if @flag.nil? %>
	$("a#flag_link_<%= @calificacion.id %>").html("Unflag");
	$("span#contador_flags_<%= @calificacion.id %>").html("<%= escape_javascript((@calificacion.flags.size + 1).to_s)%>");
<% else %>
	$("a#flag_link_<%= @calificacion.id %>").html("Flag");
	$("span#contador_flags_<%= @calificacion.id %>").html("<%= escape_javascript((@calificacion.flags.size - 1).to_s)%>");
<% end %>
